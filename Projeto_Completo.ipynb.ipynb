        "        if not self.filmes:\n",
        "            return []\n",
        "\n",
        "        diretores = [filme['diretor'] for filme in self.filmes]\n",
        "\n",
        "        contador = Counter(diretores)\n",
        "\n",
        "        if not contador:\n",
        "            return []\n",
        "\n",
        "        max_contagem = max(contador.values())\n",
        "\n",
        "        return [\n",
        "            (diretor, contagem) for diretor, contagem in contador.items()\n",
